   <script type="text/javascript"> 
    function pageSize(){
        var winWidth = $(window).width();
        var winHeight = $(window).height();
        var menuSlideHorizontal = false;
        var menuSlideWidth = 50;
        var menutopHeight = 50;
        var menuSlideHorizontalHeight = 20;
        var contentMargin = 10;
        var contentMarginT = menutopHeight;
        var searchWidth = 206;
        var modalTop = menutopHeight + contentMargin;
        //desktop
        if (winWidth>800){
            if(winWidth>1200){
                var feedWidth = winWidth*0.4;
            }
            if(1000<winWidth&&winWidth<1199){
                var feedWidth = winWidth*0.5;
            }
            if(winWidth&&winWidth<999){
                var feedWidth = winWidth*0.6;
            }
        }
        else{
            //phone horizontal
            if(570<winWidth&&winWidth<799){
                var feedWidth = winWidth-contentMargin*2-menuSlideWidth;}
            //phone vertical
            else{
                var feedWidth = winWidth-contentMargin*4;
                var menuSlideHorizontal = true;
                }
            var searchWidth = feedWidth -  135 - $("#logo").width();
            }
        
        if(menuSlideHorizontal){
            var feedWidth = feedWidth + contentMargin;
            var contentMarginL = contentMargin;
            var contentWidth = feedWidth;
            var menuSlideHeight = menuSlideHorizontalHeight;
            var modalLeft = contentMarginL;
            
            var modalHeight = winHeight - modalTop - menuSlideHorizontalHeight;
            
//             $("#content,.modal").width(feedWidth).css({marginTop:contentMargin,marginLeft:contentMarginL});
            
            
            var userWidth = 0;
            var userMargin = 0;
            var userBoxHeight = 17;
            var userBoxWidth = 70;
            
            $("#castlePrev").height(20).css({width:"auto"})
            $("#castlePrev>div").css({float:"left",marginLeft:10});
        }
        else{
            var menuSlideHeight = winHeight - menutopHeight;
            var contentWidth = feedWidth;
            var contentMarginL = menuSlideWidth + contentMargin;
            var feedWidth = contentWidth + menuSlideWidth + contentMargin;
            var modalLeft = menuSlideWidth;
            var modalHeight = winHeight - modalTop - contentMargin;
            
            var userWidth = menuSlideWidth-1;
            var userMargin = 0;
            var userBoxWidth = userWidth;
            var userBoxHeight = userBoxWidth+16;
            
            $("#castlePrev").width(userBoxWidth).css({height:"auto"})
            $("#castlePrev>div").css({float:"none",marginLeft:0});
           
        }
        var modalWidth = contentWidth;
        
        if(searchWidth<45){var searchWidth=45;}
        $('input[type="text"]').width(searchWidth)
        var feedMargin = (winWidth - feedWidth)*0.5-contentMargin;
        var modalLeft = modalLeft + feedMargin;
        $("#content").width(contentWidth).css({marginLeft:contentMarginL,marginTop:contentMarginT});
        $(".modal").height(modalHeight).width(modalWidth).css({left:modalLeft,top:modalTop});
        $("#charInfo_tab_content").height(modalHeight-60);
        $("#feed").width(feedWidth).css({marginLeft:feedMargin});
        $("#menutop>ul").css({marginRight:feedMargin});
        $("#menuSlide").width(menuSlideWidth).height(menuSlideHeight);
        $("#user img").attr({width:userWidth,height:userWidth}).css({marginLeft:userMargin});
        $("#user").height(userBoxHeight).width(userBoxWidth)        
        
//         var tabContHeight = $(".charInfo").height()-40-40;
//         $("#char_castle>.tabbable>.tab-content").height(tabContHeight);
        
        

//         $("#logo").text(winHeight)
        }
        
    
    pageSize()
    $(window).resize(function(){pageSize();});
   </script>
 
</body>
</html>